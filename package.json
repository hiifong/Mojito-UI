  },
  "engines": {
    "node": ">=20.17.0"
  },
  "dependencies": {
    "@mdit/plugin-img-lazyload": "^0.12.0",
    "@primevue/themes": "4.0.0-rc.2",
    "@vueuse/core": "^10.11.1",
    "axios": "^1.7.6",
    "cal-heatmap": "^4.2.4",
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.13",
    "element-plus": "^2.8.1",
    "floating-vue": "^5.2.2",
    "markdown-it-emoji": "^3.0.0",
    "markdown-it-footnote": "^4.0.0",
    "md-editor-v3": "^4.19.2",
    "pinia": "^2.2.2",
    "pinia-plugin-persistedstate": "^3.2.3",
    "primevue": "4.0.0-rc.2",
    "qiniu-js": "^3.4.2",
